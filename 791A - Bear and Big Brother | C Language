#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
 
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
 
 
int main(int argc, char *argv[]) {
	int a,b;
	scanf("%d %d",&a,&b);
	//4 7 12 14 36 28
	
	for(int i=1;i<100;i++)
	{
		if((pow(3,i)*a)>(pow(2,i)*b))
		{
			printf("%d",i);
			break;
		}
		
	}
	
	
	
	
	
	return 0;
}
