#include <stdio.h>
#include <stdlib.h>
#include <math.h>
 
 
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
 
int main(int argc, char *argv[]) {
	int matrix[5][5];
	int move;
	for(int i =0;i<5;i++)
	{
		for(int j =0;j<5;j++)
	{
		scanf("%d",&matrix[i][j]);
		if(matrix[i][j]==1)
		{
			move= abs(i+1-3)+abs(j+1-3);
			break;
		}
	}
	}
	printf("%d",move);
	
	
	
	
	return 0;
}
