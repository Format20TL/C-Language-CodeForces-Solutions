#include <stdio.h>
#include <stdlib.h>
#include <string.h>
 
 
int main(int argc, char *argv[]) {
	
int students,height,width,studentHeight=0;
width=0;
 
scanf("%d %d",&students,&height);
 
for(int i =0;i<students;i++)
{
	scanf("%d",&studentHeight);
	if(studentHeight>height)
	{
		width+=2;
	}
	else
	{
		width++;
	}
}
	printf("%d",width);
	return 0;
}
