#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main(int argc, char *argv[]) {
	int people;
	int opinion;
	int total;
	scanf("%d",&people);
	
	for(int i=0;i<people;i++)
	{
		scanf("%d",&opinion);
		
		if(opinion ==1)
		{
			printf("HARD");
			return 0;
		}
	}
	printf("EASY");
	
	
	
	return 0;
}
